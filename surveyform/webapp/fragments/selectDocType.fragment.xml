<?xml version="1.0" encoding="UTF-8"?>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified">
	<Dialog title="{= ${oVisibilityModel>/isdefaultLan}?${oi18n_En>selDocType}:${oi18n_En>selDocType} + '/&#160;' +${i18n>selDocType}}" contentWidth="25%"  class="sapUiSizeCompact">
		<content>
        <HBox>
        <VBox class="sapUiTinyMarginTop sapUiSmallMarginBegin">
        <Label text="{= ${oVisibilityModel>/isdefaultLan}?${oi18n_En>docType}:${oi18n_En>docType} + '/&#160;' +${i18n>docType}}" required="true"/>
            	<ComboBox width="50%" selectedKey="w8" class="inputStyle" change="fnDocTypeChange">
								<core:Item key="w8" text="W8"/>
								<core:Item key="w9" text="W9"/>
                                <core:Item key="other" text="Other"/>
                  </ComboBox>
            </VBox>
            </HBox>
			<OverflowToolbar class="barClass sapUiTinyMarginTop">
            	<ToolbarSpacer/>
				<Button class="cl_VRActnBtnClear" text="{= ${oVisibilityModel>/isdefaultLan}?${oi18n_En>cancel}:${oi18n_En>cancel} + '/&#160;' +${i18n>cancel}}" press="fnClose" tooltip="{i18n>cancel}"/>
				<!-- <Button  class="cl_VRActnBtnApply" text="{= ${oVisibilityModel>/isdefaultLan}?${oi18n_En>done}:${oi18n_En>done} + '/&#160;' +${i18n>done}}" press="fnClose" tooltip="{i18n>done}"/> -->
			<u:FileUploader id="fileUploader" buttonOnly="true" fileType="[doc,xls,pdf,jpeg,png,gif,docx,xlsx]" typeMissmatch="fnHandleTypeMissmatch" buttonText="{= ${oVisibilityModel>/isdefaultLan}?${oi18n_En>done}:${oi18n_En>done} + '/&#160;' +${i18n>done}}" multiple="true"
					useMultipart="true" change="fnOnFileUploadBP" sameFilenameAllowed="true" enabled="{oEnableMdl>/bpInfoAttachmentEnb}"
					class="cl_VRActnBtnApply">
                    <!-- <u:tooltip>
                        <c:Tooltip id="textTooltip"
                        text="{i18n>DocumentTooltip}"
                        offset="10 -5"
                        closeDuration="0"
                        />
                    </u:tooltip> -->
                    </u:FileUploader>
            </OverflowToolbar>
		</content>
	</Dialog>
</core:FragmentDefinition>