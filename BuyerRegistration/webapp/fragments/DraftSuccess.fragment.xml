<?xml version="1.0" encoding="UTF-8"?>
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="{i18n>BPCVendorRegistration}" contentWidth="30%"  class="sapUiSizeCompact APW_DilogClass">
		<content>
        <VBox alignContent="Center" alignItems="Center" >
		    <Image src="Image/Success.svg" class="sapUiSmallMarginTop cl_SuccuessImageCl"></Image>
            <Label text="{i18n>BPCSaveAsDraftMessage}" class="sapUiTinyMarginTop cl_SuccessMessageTxt" ></Label>
            </VBox>
			<OverflowToolbar class="barClass sapUiTinyMarginTop">
            	<ToolbarSpacer/>
				<Button class="cl_VRActnBtnClear" text="{i18n>BPCCancel}"  press="fnCloseMessage" tooltip="{i18n>BPCCancel}"/>
				<Button  class="cl_VRActnBtnApply" text="{i18n>BPCDone}" press="fnDoneSubmit" tooltip="{i18n>BPCDone}"/>
			</OverflowToolbar>
		</content>
	</Dialog>
</core:FragmentDefinition>